extends layout

block content
  .write_container
    form(action='/create_link', method='post', enctype='multipart/form-data')
      input#create_input(type='text', name='title', placeholder='Title')
      input#create_input(type='text', name='description', placeholder='Description')
      input#create_input(type='text', name='link', placeholder='Link')
      input#create_input(type='password', name='password', placeholder='Password')
      input(type='submit')
